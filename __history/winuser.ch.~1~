    #define EN_VSCROLL            1538

    //#if(_WIN32_WINNT >=   1280)
    #define EN_ALIGN_LTR_EC       1792
    #define EN_ALIGN_RTL_EC       1793
    //#endif // _WIN32_WINNT >= 0x0500

    //#if(WINVER >=   1024)
