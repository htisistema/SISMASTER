****************
* MENU PRINCIPAL
****************
FUNCTION sac0()
***************
MEMVAR mdata_sis,memp_resa,mversao,cod_operado,mcaminho,mol,sistema
LOCAL opcao,tela,aret:={},i:=0,mdt1,mdt2,mc_prod
PRIVATE mcod_vend,mmens,cons_rot:={},hMainMenu,mtela_principal

SETMODE(50,120)
SETCOLOR('W/B,B/w,,,GR/B')
op_tela(00,00,50,120,memp_resa+SPACE(20)+'.: SISCOM '+mversao+' :.'+SPACE(20)+'Data:'+DTOC(mdata_sis)+' - '+ver_dia(mdata_sis) )
wvw_maximize(0)
WVW_CenterWindow(,.T.,.T.)

SETCOLOR('W+/B+,B/w,,,GR+/B')
WVW_DrawImage( ,00,00,50,MAXCOL(),ALLTRIM(m_cfg[5])+'HRBLOGO.JPG',.T.,.F.)
IF FILE(ALLTRIM(m_cfg[5])+'HRBFIRMA.JPG')
        WVW_DrawImage( ,02,04,22,74,ALLTRIM(m_cfg[5])+'HRBFIRMA.JPG',.T.,.F.)
ENDIF
IF FILE(ALLTRIM(m_cfg[63])+cod_operado+'.jpg')
        WVW_DrawImage( ,00,88,06,100,ALLTRIM(m_cfg[63])+cod_operado+'.jpg',.T.,.F.)
        WVW_DrawLabel(,07,93,'OPERADOR',,,,, 'terminal',13,4,,,,,)
ENDIF
mtela_principal := WVW_SAVESCREEN(,00,00,MAXROW(),MAXCOL())
exibi_prg('SAC0')
set key 23 to
opcao:=0
/*
IP_EXTERNO()
buscar_endereco()
*/
SET KEY 28 TO menu_cons()       // F1 MENU DE CONSULTAS
//SET KEY -1 TO menu_cons()     // F2 MENU DE CONSULTAS
SET KEY -30 TO cons_mens()      // ALT+F1
SET KEY 287 TO alt_senha()      // ALT+S
SET KEY 289 TO sacc_fin()       // ALT+F
SET KEY 276 TO alt_data()       // ALT+T
//SET KEY 274 TO alt_emp()      // ALT+E   ALTERACAO DE EMPRESA
SET KEY 408 TO fim_prg()        // ALT+seta p/cima
SET KEY 300 TO fim()            // ALT+Z fim do sistema
SET KEY -40 TO calend()         // F11
SET KEY -41 TO calcu()          // F12 calculadora
SET KEY 278 TO autorizar()      // ALT+U (SACROT)
SET KEY 270 TO senha_cesso()    // ALT+SETA ESQUEDA
SET KEY 280 TO sac3411()        // ALT+O VERIFICA PEDIDOS DO POCKETS
SETCOLOR('W/B+,B/w,,,GR+/B')
IF m_set[1,102] = 'S'
        IF ! AbriArq('sacdolar','dolar');RETURN NIL;ENDIF
        ver_dolar(mdata_sis,'*')
        *****************************
        SELE('dolar');ORDSETFOCUS(1)
        GO BOTT
        *****************************
ENDIF
SET KEY 287 TO alt_senha        // ALT+S
ver_mensa(cod_operado)          // verifica mensagens

//WVW_TBCreate( , .T.,TBSTYLE_TOOLTIPS        , 0,10,10)
//WVW_TBCreate( , .T.,TBSTYLE_WRAPABLE        , 0,20,20)
//WVW_TBCreate( , .T.,TBSTYLE_ALTDRAG         , 0,20,20)
//WVW_TBCreate( , .T.,TBSTYLE_FLAT            , 0,20,20)
WVW_TBCreate( , .T.,TBSTYLE_LIST            , 0,3,3)

IF m_set[1,157] # 'PROVEDOR'
        WVW_TBAddButton( ,NBotao1,' ','<V> E N D A',1)
        WVW_TBAddButton( ,NBotao2,' ','<R>ecebimento',1)
        WVW_TBAddButton( ,NBotao3,' ','<B>aixa Doc.',1)
        WVW_TBAddButton( ,NBotao4,' ','<O>rcamento',1)
        WVW_TBAddButton( ,NBotao5,' ','<P>rodutos',1)
        WVW_TBAddButton( ,NBotao6,' ','<M>ensagem',1)
        WVW_TBAddButton( ,NBotao7,' ','<D> Senha',1)
        WVW_TBAddButton( ,NBotao8,' ','<C>lientes',1)
        WVW_TBAddButton( ,NBotao9,' ','<ESC> Sair',1)
ELSE
        WVW_TBAddButton( ,NBotao1,' ','O.S. Instalacao',1)
        WVW_TBAddButton( ,NBotao2,' ','Baixa O.S.(Inst)',1)
        WVW_TBAddButton( ,NBotao3,' ','<B>aixa Doc.',1)
        WVW_TBAddButton( ,NBotao4,' ','O.S. Manutencao',1)
        WVW_TBAddButton( ,NBotao5,' ','Baixa O.S.(Man)',1)
        WVW_TBAddButton( ,NBotao6,' ','<M>ensagem',1)
        WVW_TBAddButton( ,NBotao7,' ','<D> Senha',1)
        WVW_TBAddButton( ,NBotao8,' ','<C>lientes',1)
        WVW_TBAddButton( ,NBotao9,' ','<ESC> Sair',1)
ENDIF
// 110 250 180 150 290 150
WVW_SBCreate()
WVW_SBAddPart(,,150,0,.F.)
WVW_SBSetText(,0,'{SAC0}')
WVW_SBAddPart(,,150,0,.F.)
WVW_SBSetText(,1,'Empresa: '+ALLTRIM(mcodempresa)+'-'+memp_resa)
WVW_SBAddPart(,,150,0,.F.)
WVW_SBSetText(,2,'Operador: '+ALLTRIM(cod_operado))
WVW_SBAddPart(,,150,0,.F.)
WVW_SBSetText(,3,'Term.: '+NETNAME())
WVW_SBAddPart(,,150,0,.F.)
IF ! EMPTY(m_set[1,82])
	IF cod_operado = '999'
        	WVW_SBSetText(,4,'   ** Banco: FIREBIRD ** '+STRZERO(DATE() - CTOD(SUBSTR(m_set[1,82],1,2)+'/'+SUBSTR(m_set[1,82],3,2)+'/'+SUBSTR(m_set[1,82],5,2)),3))
	ELSE
        	WVW_SBSetText(,4,'   ** Banco: FIREBIRD **')
        ENDIF
ELSE
        WVW_SBSetText(,4,'   >> Banco: FIREBIRD <<')
ENDIF
WVW_SBAddPart(,,50,0,.F.)
WVW_SBSetText(,5,'IP Term.: '+GetIp())
WVW_SBAddPart(,,50,0,.F.)
WVW_SBSetText(,6,'IP Serv.: '+ALLTRIM(m_cfg[84]))
//WVW_SBAddPart(,,80,0,.F.)
//WVW_SBSetText(,5,ALLTRIM(m_cfg[84]))

/*
IF ! EMPTY(m_set[1,82])
	IF cod_operado = '999'
        	WVW_SBSetText(,6,'IP:'+getip()+'   ** Banco: FIREBIRD ** '+STRZERO(DATE() - CTOD(SUBSTR(m_set[1,82],1,2)+'/'+SUBSTR(m_set[1,82],3,2)+'/'+SUBSTR(m_set[1,82],5,2)),3))
	ELSE
        	WVW_SBSetText(,6,'IP:'+getip()+'   ** Banco: FIREBIRD **')
        ENDIF
ELSE
        WVW_SBSetText(,6,'IP:'+getip()+'   >> Banco: FIREBIRD <<')
ENDIF
*/
mdias_fecha := CTOD(SUBSTR(m_set[1,82],1,2)+'/'+SUBSTR(m_set[1,82],3,2)+'/'+SUBSTR(m_set[1,82],5,2)) - DATE()
IF mdias_fecha <= 5 .AND. ! EMPTY(m_set[1,82])   //.AND. ! EMPTY(CTOD(SUBSTR(m_set[1,82],1,2)+'/'+SUBSTR(m_set[1,82],3,2)+'/'+SUBSTR(m_set[1,82],5,2)))
        IF (mdias_fecha <= 0 .OR. SUBSTR(m_set[1,82],12,1) = '*') .AND. m_set[1,158] = 'M'
                atencao('FAVOR ENTRA EM CONTATO COM A " HRB Sistemas - Telefone:(81)3755.1489-3325.6663" PARA QUE SEJA RESOLVIDO ESTE PROBLEMA DE TABELAS CORROMPIDAS !!!')
                fim('SISCOM','*')
                RETURN NIL
        ENDIF
        IF m_set[1,158] = 'M'
                atencao('Estar faltando '+STRZERO(mdias_fecha,2)+' dias para Expirar o sistema, Entre em contato com a "HRB Sistemas  - Telefone:(81)3755.1489-3325.6663" para pegar a senha de liberacao !!!')
        ELSE
                atencao('Existe alguma(s) TABELA com problema, Entre em contato com a "HRB Sistemas  - Telefone:(81)3755.1489-3325.6663" para que possa resolver antes que tenha um problema maior !!!')
        ENDIF
ENDIF
hMainMenu   = WVW_CreateMenu( )
hSubMenu1   = WVW_CreateMenu( )

hSubMenu2   = WVW_CreateMenu( )
hSubMov2_8  = WVW_CreateMenu( )
hSubMov2_9  = WVW_CreateMenu( )
hSubMov2_12 = WVW_CreateMenu( )
hSubMov2_16 = WVW_CreateMenu( )
hSubMov2_17 = WVW_CreateMenu( )
hSubMov2_17_1 = WVW_CreateMenu( )
hSubMov2_19 = WVW_CreateMenu( )
hSubMov2_19_1 = WVW_CreateMenu( )
hSubMov2_25 = WVW_CreateMenu( )
hSubMov2_26 = WVW_CreateMenu( )
hSubMov2_27 = WVW_CreateMenu( )
hSubMov2_28 = WVW_CreateMenu( )
hSubMov2_29 = WVW_CreateMenu( )
hSubMov2_291 = WVW_CreateMenu( )

hPEDMenu2   = WVW_CreateMenu( )
hORCMenu2   = WVW_CreateMenu( )
hNFMenu1   = WVW_CreateMenu( )
hNFMenu2   = WVW_CreateMenu( )
hNFMenu3   = WVW_CreateMenu( )
hHVMenu2   = WVW_CreateMenu( )
hDUPPMenu2   = WVW_CreateMenu( )
hDUPRMenu2   = WVW_CreateMenu( )
