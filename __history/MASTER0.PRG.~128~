IF mdias_fecha <= 5 .AND. ! EMPTY(m_set[1,15])   //.AND. ! EMPTY(CTOD(SUBSTR(m_set[1,15],1,2)+'/'+SUBSTR(m_set[1,15],3,2)+'/'+SUBSTR(m_set[1,15],5,2)))
