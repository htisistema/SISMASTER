nPBid:={}
i:=0
FOR i = 1 TO 121
        AADD(nPBid,0)
NEXT
m_sair := 0
//WVW_PBSetFont( nWinNum, 'Arial Black',80,40, nWeight, nQuality, lItalic, lUnderline,lStrikeout )

